                    android:layout_width="200dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginBottom="10dp"
                    android:onClick="changeToAnnual"
                    android:text="Jahresansicht"
                    android:backgroundTint="@color/Button"
                    android:textStyle="bold"
                    android:textColor="@color/Textcolorbright1"

                    />

                <Button
                    android:id="@+id/changeViewMonthcomparison"
                    android:layout_width="200dp"
                    android:layout_height="100dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginBottom="100dp"
                    android:onClick="changeToMonthcomparison"
                    android:text="Montatsvergleich"
                    android:backgroundTint="@color/Button"
                    android:textStyle="bold"
                    android:textColor="@color/Textcolorbright1"

                    />

            </LinearLayout>


        </androidx.cardview.widget.CardView>
